{
    "version": 2,
    "functions": {
        "api/server.php": {
            "runtime": "vercel-php@0.7.1",
            "excludeFiles": "{public/images/**,public/sound/**,node_modules/**,tests/**}"
        }
    },
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "/public/build/$1"
        },
        {
            "src": "/(css|js|images|sound)/(.*)",
            "dest": "/public/$1/$2"
        },
        {
            "src": "/(favicon.ico|robots.txt)",
            "dest": "/public/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/server.php"
        }
    ],
    "env": {
        "APP_NAME": "Laravel",
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie",
        "CACHE_STORE": "array"
    }
}
